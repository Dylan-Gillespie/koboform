<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='KoBoFormDesigner'>
	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name='com.google.gwt.user.User' />

	<!-- Inherit the default GWT style sheet.  You can change       -->
	<!-- the theme of your GWT application by uncommenting          -->
	<!-- any one of the following lines.                            -->
	<!-- <inherits name='com.google.gwt.user.theme.standard.Standard' /> -->
	<inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

	<!-- Other module inherits                                      -->
	<inherits name='org.purc.purcforms.FormDesigner' />
	<inherits name='org.purc.purcforms.SharedLib' />
	<inherits name='com.extjs.gxt.ui.GXT' />
	<inherits name='com.extjs.gxt.themes.Themes' />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<inherits name="com.google.code.gwt.storage.Html5Storage" />
	<inherits name="com.google.gwt.i18n.I18N"/>
	<!-- Inherits for JavaRosa stuff  
	<inherits name="org.javarosa.JavaRosaXform"/>
	<inherits name="org.javarosa.JavaRosaCore"/>
	<inherits name="org.kxml2.KXml2"/>
	<inherits name="org.openrosa.gwt.resources.gwt.GWT_JRE"/>
	-->

	<!-- Specify the app entry point class.                         -->
	<entry-point class='org.openrosa.client.FormDesigner' />

	<!-- Specify the paths for translatable code                    -->
	<source path='client' />

	<!-- Properties -->
	<set-property name="user.agent" value="gecko1_8,safari,ie6" />
	<set-property name="locale" value="default" />
	
	<!-- For implementing JRE classes that GWT doesn't have so that Java Rosa will work -->
	
	
</module>
